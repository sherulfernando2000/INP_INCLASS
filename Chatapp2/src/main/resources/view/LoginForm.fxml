<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="163.0" prefWidth="197.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.controller.LoginFormController">
<children>
    <VBox layoutX="1.0" layoutY="1.0" prefHeight="150.0" prefWidth="214.0">
        <children>
            <Label prefHeight="21.0" prefWidth="204.0" text=" Welcome to SDF Chat" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <TextField fx:id="txtUserName" prefHeight="25.0" prefWidth="195.0">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin></TextField>
            <Button mnemonicParsing="false" onAction="#btnLoginOnAction" text="Login">
                <VBox.margin>
                  <Insets left="60.0" top="20.0" />
                </VBox.margin>
            </Button>
        </children>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
    </VBox>
</children>
</AnchorPane>
